@model IEnumerable<GitAspx.ViewModels.RepositoryBaseViewModel>
@using GitAspx;

@{
    ViewBag.Title = "Main";
}

<div class="repositoryContainer">
<h1>Main</h1>
<table>
    <tr>
        <th>
            Name
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Name, "List", new { store=item.Name }) 
        </td>
    </tr>
}

</table>
</div>